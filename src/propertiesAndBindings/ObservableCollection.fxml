<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="208.0" prefWidth="304.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="propertiesAndBindings.ObservableCollection">
    <children>
        <VBox layoutX="170.0" layoutY="71.0" prefHeight="204.0" prefWidth="222.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox spacing="5.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#addName" text="Add"/>
                        <TextField fx:id="nameTF"/>
                        <Button mnemonicParsing="false" onAction="#removeName" text="Remove"/>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </VBox.margin>
                </HBox>
                <ListView fx:id="nameLV">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0"/>
                    </VBox.margin>
                </ListView>
            </children>
        </VBox>
    </children>
</AnchorPane>
